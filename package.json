    "@typescript-eslint/parser": "^1.4.2",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.16.0",
    "jest": "24.9.0",
    "lint-staged": "^7.0.4",
    "prettier": "^1.12.0",
    "react-animated-css": "^1.0.4",
