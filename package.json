    "nodemailer": "^5.1.1",
    "nodemon": "^1.19.4",
    "react": "^16.8.3",
    "react-animated-css": "^1.0.4",
    "react-dom": "^16.8.3",
    "react-scripts": "^3.2.0",
    "reactstrap": "^7.1.0",
